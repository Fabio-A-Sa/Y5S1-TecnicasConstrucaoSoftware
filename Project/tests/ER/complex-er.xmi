<?xml version="1.0" encoding="UTF-8"?>
<project.er.ERModel:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:project.er.ERModel="http://project/er/ERModel"
    name="Project Management System">
  <entities
      ename="Project">
    <attributes
        aname="id"
        type="Integer"/>
    <attributes
        aname="name"
        type="String"/>
    <attributes
        aname="description"
        type="String"/>
    <attributes
        aname="status"
        type="String"/>
    <attributes
        aname="start_date"
        type="Datetime"/>
    <attributes
        aname="end_date"
        type="Datetime"/>
  </entities>
  <entities
      ename="Team">
    <attributes
        aname="id"
        type="Integer"/>
    <attributes
        aname="name"
        type="String"/>
  </entities>
  <entities
      ename="Member">
    <attributes
        aname="id"
        type="Integer"/>
    <attributes
        aname="name"
        type="String"/>
    <attributes
        aname="role"
        type="String"/>
    <attributes
        aname="email"
        type="String"/>
  </entities>
  <entities
      ename="Task">
    <attributes
        aname="id"
        type="Integer"/>
    <attributes
        aname="description"
        type="String"/>
    <attributes
        aname="status"
        type="String"/>
    <attributes
        aname="start_date"
        type="Datetime"/>
    <attributes
        aname="end_date"
        type="Datetime"/>
  </entities>
  <entities
      ename="Resource">
    <attributes
        aname="id"
        type="Integer"/>
    <attributes
        aname="name"
        type="String"/>
    <attributes
        aname="resource_type"/>
    <attributes
        aname="description"
        type="String"/>
  </entities>
  <relationships
      rname="belongsTo"
      entities="//@entities.0 //@entities.1"
      upperBoundA="-1"
      lowerBoundB="1"
      upperBoundB="-1"/>
  <relationships
      rname="has"
      entities="//@entities.1 //@entities.2"
      lowerBoundA="1"
      upperBoundA="1"
      lowerBoundB="1"
      upperBoundB="-1"/>
  <relationships
      rname="does"
      entities="//@entities.1 //@entities.3"
      lowerBoundA="1"
      upperBoundA="1"
      upperBoundB="-1"/>
  <relationships
      rname="requires"
      entities="//@entities.3 //@entities.4"
      upperBoundA="-1"
      upperBoundB="-1"/>
</project.er.ERModel:Model>
